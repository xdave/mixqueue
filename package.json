    "sinon": "^2.2.0",
    "source-map": "^0.5.6",
    "ts-node": "^3.0.3",
    "tslib": "^1.6.1",
    "typescript": "^2.3.2",
    "uglify-js": "^2.8.22"
  },
  "dependencies": {
    "aphrodite": "^1.2.1",
    "fetch-jsonp": "^1.0.6",
    "lodash": "^4.17.4",
    "material-ui": "next",
