{
  "name": "mixqueue",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run remove-assets && npm run copy-dist && npm run copy-favicon",
    "preview": "vite preview",
    "prod": "cross-env NODE_ENV=production npm run build",
    "start": "npm run -s dev",
    "watch": "npm run -s dev",
    "remove-assets": "rm -rf docs/assets",
    "copy-dist": "rsync -ahP dist/ docs/",
    "copy-favicon": "cp -v docs/favicon.ico docs/assets/"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-enzyme": "^0.6.9",
    "@types/classnames": "^2.3.0",
    "@types/enzyme": "^3.10.12",
    "@types/glob": "^8.0.0",
    "@types/history": "^4.7.11",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^3.0.2",
    "@types/prop-types": "^15.7.5",
    "@types/query-string": "^6.3.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/recompose": "^0.30.10",
    "@types/redux-immutable-state-invariant": "^2.1.2",
    "@types/redux-logger": "^3.0.9",
    "@types/sinon": "^10.0.13",
    "@vitejs/plugin-react": "^3.0.1",
    "chai": "^4.3.7",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "cue-parser-plus": "^0.0.5",
    "enzyme": "^3.11.0",
    "fuse-box": "^4.0.1-next.4",
    "mocha": "^10.8.2",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "uglify-js": "^3.17.4",
    "vite": "^4.5.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.20.7",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.3",
    "@mui/styles": "^5.11.2",
    "classnames": "^2.3.2",
    "fetch-jsonp": "^1.2.3",
    "history": "^5.3.0",
    "jss-preset-default": "^10.9.2",
    "react": "^18.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^18.2.0",
    "react-jss": "^10.9.2",
    "react-redux": "^8.0.5",
    "react-router": "^6.6.1",
    "react-router-dom": "^6.6.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.7",
    "smoothscroll-polyfill": "^0.4.4",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2",
    "typescript-fsa-redux-thunk": "^2.10.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "src/**/*.spec.*",
      "src/**/*.test.*"
    ],
    "extension": [
      ".ts",
      ".tsx",
      ".js",
      ".jsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}